{
  "app": {
    "name": "Creative Writing Verification Interactive",
    "version": "1.0.0",
    "debugMode": false,
    "environment": "production"
  },
  
  "features": {
    "authentication": {
      "enabled": true,
      "mode": "hybrid",
      "disableForAnonymous": false,
      "requireEmailConfirmation": false,
      "allowAnonymousAccess": true
    },
    "dashboard": {
      "enabled": true,
      "showLink": true
    },
    "leaderboard": {
      "enabled": true,
      "showDateFilters": false
    },
    "resources": {
      "enabled": true
    },
    "dataset": {
      "enabled": true,
      "downloadUrl": null
    },
    "ui": {
      "showHelpButton": true,
      "enableDarkMode": false,
      "defaultTheme": "light"
    }
  },
  
  "models": {
    "defaultProvider": "openai",
    "defaultTemperature": 0.7,
    "defaultMaxTokens": 1024,
    "defaultReasoningEffort": "medium",
