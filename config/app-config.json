  "api": {
    "rateLimit": {
      "enabled": true,
      "userMaxRequests": 100,
      "anonMaxRequests": 20,
      "windowMs": 900000
    },
    "caching": {
      "enabled": true,
      "ttlMs": 300000
    }
  },
  
